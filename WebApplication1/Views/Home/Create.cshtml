@{
    ViewBag.Title = "Create Order";
}
<h2>@ViewBag.Title.</h2>

<form method="post">
    <select name="MenuItem">
        @foreach (var menuItem in ViewBag.MenuItems)
        {
            <option value="@menuItem.Id">@menuItem.Name</option>
        }
    </select>
    <input type="submit" value="Submit" />
</form>
