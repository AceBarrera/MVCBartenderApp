@{
    ViewBag.Title = "Orders";
}
<h2>@ViewBag.Title.</h2>
<ul>
    @foreach (var order in ViewBag.Orders)
    {
        <li>
            @order.MenuItem.Name
            <form style="display: inline-block;" method="post" action="/Order/Complete/@order.Id"><input value="Complete" type="submit" /></form>
            <form style="display: inline-block;" method="post" action="/Order/Delete/@order.Id"><input value="Delete" type="submit" /></form>
        </li>
    }
    @if(ViewBag.Count == 0)
    {
        <p>hAy</p>
    }
</ul>

<p>Use this area to provide additional information.</p>
